        <activity android:name=".detail" />
        <activity android:name=".cartact" />
        <activity android:name=".updatequantaty" />
        <activity android:name=".barcode"></activity>
        <meta-data
            android:name="com.google.android.gms.vision.DEPENDENCIES"
